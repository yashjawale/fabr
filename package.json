{
	"name": "fabr",
	"version": "0.0.2",
	"description": "A command line utility for scaffolding new development projects ",
	"keywords": [
		"scaffold",
		"starter",
		"template",
		"bootstrap",
		"fabr"
	],
	"homepage": "https://yashjawale.github.io/fabr",
	"bugs": {
		"url": "https://github.com/yashjawale/fabr/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/yashjawale/fabr.git"
	},
	"license": "GPL-3.0-or-later",
	"author": "Yash Jawale",
	"type": "module",
	"main": "dist/index.js",
	"bin": {
		"fabr": "dist/index.js"
	},
	"files": [
		"dist/",
		"README.md",
		"LICENSE"
	],
	"publishConfig": {
		"access": "public"
	},
	"engines": {
		"node": ">=18.0.0"
	},
	"scripts": {
		"build": "tsc",
		"start": "npm run build && node dist/index.js",
		"dev": "tsx watch src/index.ts",
		"lint": "eslint src/**/*.ts",
		"lint:fix": "eslint src/**/*.ts --fix",
		"format": "prettier --write .",
		"format:check": "prettier --check .",
		"typecheck": "tsc --noEmit",
		"ci": "npm run lint && npm run format:check && npm run typecheck && npm run build",
		"test:cli": "npm run build && node dist/index.js --help && node dist/index.js list",
		"semantic-release": "semantic-release"
	},
	"dependencies": {
		"@inquirer/prompts": "^7.8.0",
		"chalk": "^5.4.1",
		"execa": "^9.6.0",
		"ora": "^9.0.0",
		"typescript-eslint": "^8.39.0"
	},
	"devDependencies": {
		"@eslint/js": "^9.17.0",
		"@types/node": "^24.1.0",
		"eslint": "^9.17.0",
		"eslint-config-prettier": "^10.1.8",
		"prettier": "^3.4.2",
		"semantic-release": "^25.0.1",
		"ts-node-dev": "^2.0.0",
		"tsx": "^4.20.3",
		"typescript": "^5.6.0"
	}
}
