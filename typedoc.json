{
	"$schema": "https://typedoc.org/schema.json",
	"entryPoints": ["src/lib/*.ts", "src/types/*.ts"],
	"entryPointStrategy": "expand",
	"out": "website/src/content/docs/docs/developer/api",
	"plugin": ["typedoc-plugin-markdown", "typedoc-plugin-frontmatter"],
	"readme": "none",
	"hideGenerator": true,
	"excludePrivate": true,
	"excludeProtected": true,
	"excludeInternal": true,
	"sort": ["source-order"],
	"groupOrder": ["Functions", "Classes", "Interfaces", "Type Aliases", "Variables"],
	"categorizeByGroup": false,
	"kindSortOrder": [
		"Project",
		"Module", 
		"Function",
		"Class",
		"Interface",
		"TypeAlias",
		"Variable"
	],
	"visibilityFilters": {
		"protected": false,
		"private": false,
		"@internal": false
	},
	"searchInComments": true,
	"cleanOutputDir": true,
	"disableSources": false,
	"sourceLinkTemplate": "https://github.com/yashjawale/fabr/blob/{gitRevision}/{path}#L{line}",
	"gitRevision": "main",
	"frontmatterGlobals": {
		"description": "Auto-generated API documentation"
	},
	"frontmatterCommentTags": ["title"]
}
